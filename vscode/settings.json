{
  "window.title": "${dirty}${activeEditorMedium}${separator}${folderName}${separator}${appName}",
  "window.menuBarVisibility": "default",
  "window.openFoldersInNewWindow": "on",
  "window.zoomLevel": 0,
  "workbench.iconTheme": "file-icons",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.activityBar.visible": true,
  "workbench.statusBar.visible": true,
  // For Ubuntu.
  "editor.fontFamily": "Operator Mono Medium",
  // For Windows.
  // "editor.fontFamily": "Operator Mono Light",
  "editor.fontSize": 17,
  "editor.fontWeight": "400",
  "editor.lineHeight": 32,
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "same",
  "editor.tabCompletion": true,
  "editor.letterSpacing": 0.2,
  "editor.tabSize": 2,
  "editor.cursorStyle": "line",
  "editor.fontLigatures": true,
  "editor.renderWhitespace": "none",
  "editor.selectionHighlight": false,
  "editor.cursorBlinking": "phase",
  "editor.colorDecorators": false,
  "editor.matchBrackets": false,
  "editor.autoIndent": true,
  "editor.renderIndentGuides": true,
  "editor.renderLineHighlight": "all",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": false,
  "explorer.autoReveal": false,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1200,
  "atomKeymap.promptV3Features": true,
  "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\cmd.exe",
  "subtleBrackets.styles": {
    "global": {
      "borderWidth": "2px",
      "borderStyle": "none none solid none",
      "borderColor": "#7e57c2"
    }
  },
  "colorize.activate_variables_support_beta": true,
  "emmet.showExpandedAbbreviation": "always",
  "emmet.triggerExpansionOnTab": true,
  "emmet.showAbbreviationSuggestions": true,
  "emmet.showSuggestionsAsSnippets": true,
  "terminal.integrated.fontFamily": "Ubuntu Mono",
  "files.insertFinalNewline": true,
  "editor.smoothScrolling": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "editor.emptySelectionClipboard": false,
  "workbench.colorTheme": "Material Palenight for Operator Mono",
  "workbench.commandPalette.preserveInput": true,
  "files.hotExit": "onExitAndWindowClose"
}
