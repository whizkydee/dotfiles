{
  "window.title": "${dirty}${activeEditorMedium}${separator}${folderName}${separator}${appName}",
  "window.menuBarVisibility": "toggle",
  "window.openFoldersInNewWindow": "on",
  "window.zoomLevel": 0,
  "workbench.iconTheme": "file-icons",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.activityBar.visible": true,
  "workbench.statusBar.visible": true,
  // For Ubuntu.
  // "editor.fontFamily": "'Operator Mono Medium', 'Ubuntu Mono'",
  // For Windows.
  "editor.fontFamily": "'Operator Mono Light', 'Ubuntu Mono'",
  "editor.fontSize": 16,
  "editor.fontWeight": "400",
  "editor.lineHeight": 26,
  "editor.wordWrap": "on",
  "editor.tabCompletion": true,
  "editor.letterSpacing": 0,
  "editor.tabSize": 2,
  "editor.mouseWheelZoom": true,
  "editor.cursorStyle": "line",
  "editor.fontLigatures": false,
  "editor.renderWhitespace": "none",
  "editor.selectionHighlight": false,
  "editor.cursorBlinking": "phase",
  "editor.matchBrackets": false,
  "editor.autoIndent": true,
  "editor.renderIndentGuides": true,
  "editor.renderLineHighlight": "all",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": false,
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": false,
  "explorer.autoReveal": false,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 100,
  "atomKeymap.promptV3Features": true,
  "terminal.integrated.shell.windows": "C:\\WINDOWS\\Sysnative\\cmd.exe",
  "subtleBrackets.styles": {
    "global": {
      "borderWidth": "2px",
      "borderStyle": "none none solid none",
      "borderColor": "#7e57c2"
    }
  },
  "colorize.activate_variables_support_beta": true,
  "css.colorDecorators.enable": false,
  "scss.colorDecorators.enable": false,
  "less.colorDecorators.enable": false,
  "json.colorDecorators.enable": false,
  "emmet.showExpandedAbbreviation": "always",
  "emmet.showAbbreviationSuggestions": true,
  "workbench.colorTheme": "Material Palenight for Operator Mono",
  "emmet.triggerExpansionOnTab": true
}