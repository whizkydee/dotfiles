{
  "window.title": "${dirty}${activeEditorMedium}${separator}${folderName}${separator}${appName}",
  "window.menuBarVisibility": "toggle",
  "window.openFoldersInNewWindow": "on",
  "workbench.iconTheme": "file-icons",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.activityBar.visible": true,
  "workbench.statusBar.visible": true,
  "editor.fontFamily": "'Dank Mono',  'Fira Code', 'Ubuntu Mono'",
  "editor.fontSize": 15,
  "editor.fontWeight": "900",
  "editor.lineHeight": 42,
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "same",
  "editor.tabCompletion": true,
  "editor.letterSpacing": 0.2,
  "editor.tabSize": 2,
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 3,
  "editor.cursorBlinking": "phase",
  "editor.fontLigatures": false,
  "editor.renderWhitespace": "none",
  "editor.selectionHighlight": false,
  "editor.colorDecorators": false,
  "editor.matchBrackets": false,
  "editor.autoIndent": true,
  "editor.renderIndentGuides": true,
  "editor.renderLineHighlight": "all",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": false,
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": false,
  "explorer.autoReveal": false,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 2000,
  "atomKeymap.promptV3Features": true,
  "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\cmd.exe",
  "subtleBrackets.style": {
    "borderWidth": "2px",
    "borderStyle": "none none solid none",
    "borderColor": "#7e57c2"
  },
  "emmet.showExpandedAbbreviation": "always",
  "emmet.showAbbreviationSuggestions": true,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": "javascriptreact"
  },
  "editor.snippetSuggestions": "top",
  "prettier.singleQuote": true,
  "prettier.semi": true,
  "workbench.commandPalette.preserveInput": true,
  "files.insertFinalNewline": true,
  "git.defaultCloneDirectory": "C:\\Users\\Olaoluwa\\GitHub",
  "explorer.decorations.colors": false,
  "explorer.confirmDragAndDrop": false,
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "editor.insertSpaces": true,
  "editor.emptySelectionClipboard": false,
  "prettier.eslintIntegration": true,
  "prettier.stylelintIntegration": true,
  "editor.formatOnSave": true,
  "explorer.confirmDelete": false,
  "markdown.preview.breaks": true,
  "markdown-preview-enhanced.enableExtendedTableSyntax": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "git.autofetch": true,
  "terminal.integrated.rendererType": "dom",
  "editor.detectIndentation": false,
  "breadcrumbs.enabled": true,
  "workbench.colorTheme": "Palenight Theme",
  "extensions.ignoreRecommendations": true,
  "extensions.showRecommendationsOnlyOnDemand": true,
  "window.zoomLevel": -1.2
