{
  "window.title": "${dirty}${activeEditorMedium}${separator}${folderName}${separator}${appName}",
  "window.openFoldersInNewWindow": "on",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.activityBar.visible": true,
  "workbench.statusBar.visible": true,
  "editor.fontFamily": "Operator Mono, Fira Code, Menlo, Ubuntu Mono",
  "editor.fontSize": 15,
  "editor.lineHeight": 40,
  "editor.wordWrap": "on",
  "workbench.tree.renderIndentGuides": "none",
  "editor.wrappingIndent": "same",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 3,
  "editor.cursorBlinking": "phase",
  "editor.fontLigatures": true,
  "editor.renderWhitespace": "none",
  "editor.selectionHighlight": false,
  "editor.colorDecorators": false,
  "editor.matchBrackets": false,
  "editor.autoIndent": true,
  "editor.renderIndentGuides": true,
  "editor.renderLineHighlight": "all",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": false,
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": false,
  "explorer.autoReveal": false,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 2000,
  "atomKeymap.promptV3Features": true,
  "subtleBrackets.style": {
    "borderWidth": "2px",
    "borderStyle": "none none solid none",
    "borderColor": "#7e57c2"
  },
  "emmet.showExpandedAbbreviation": "always",
  "emmet.showAbbreviationSuggestions": true,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "editor.snippetSuggestions": "top",
  "prettier.singleQuote": true,
  "prettier.semi": false,
  "prettier.trailingComma": "es5",
  "workbench.commandPalette.preserveInput": true,
  "files.insertFinalNewline": true,
  "git.defaultCloneDirectory": "/home/olaoluwa/Projects",
  "explorer.decorations.colors": false,
  "explorer.confirmDragAndDrop": false,
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "editor.insertSpaces": true,
  "editor.emptySelectionClipboard": false,
  "prettier.eslintIntegration": true,
  "prettier.stylelintIntegration": true,
  "eslint.autoFixOnSave": true,
  "editor.formatOnSave": true,
  "explorer.confirmDelete": false,
  "markdown.preview.breaks": true,
  "markdown-preview-enhanced.enableExtendedTableSyntax": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "git.autofetch": true,
  "auto-close-tag.activationOnLanguage": [
    "xml",
    "php",
    "blade",
    "ejs",
    "jinja",
    "javascript",
    "javascriptreact",
    "typescriptreact",
    "plaintext",
    "markdown",
    "vue",
    "liquid",
    "erb",
    "lang-cfml",
    "cfml",
    "HTML (Eex)"
  ],
  "colorize.languages": [
    "css",
    "sass",
    "scss",
    "less",
    "postcss",
    "sss",
    "javascript",
    "stylus",
    "xml",
    "svg"
  ],
  "terminal.integrated.rendererType": "dom",
  "editor.detectIndentation": false,
  "breadcrumbs.enabled": true,
  "workbench.colorTheme": "Palenight Theme",
  "extensions.ignoreRecommendations": true,
  "extensions.showRecommendationsOnlyOnDemand": true,
  "vetur.format.defaultFormatter.html": "prettier",
  "window.zoomLevel": -0.8,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue"
  ],
  "editor.fontWeight": "300",
  "workbench.editor.showTabs": true,
  "git.enableSmartCommit": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
    "[Palenight Theme]": {
      "titleBar.activeBackground": "#292d3e",
      "titleBar.activeForeground": "#eeefff",
      "titleBar.border": "#30364c"
    }
  },
  "workbench.iconTheme": "file-icons"
}
